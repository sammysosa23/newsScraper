<div>
  <h1 class="card z-depth-3 center-align">Get some News!</h1>
  <div class="row">
    <div class="col s3"></div>
    <br>
    <ul><a class="waves-effect waves-light btn scrapeButton" href="/scrape">Scrape</a></ul>
    <ul><a class="waves-effect waves-light btn clearButton" href="/clearall">Clear</a></ul>
  </div>
</div>
<div class="row">
  <div class="col s2"></div>
  <div class="col s8 left-align">
    {{#each article}}
    <div class="card-panel">
      <h4>{{this.title}}</h4>
      <a href="/readArticle/{{this._id}}" class="waves-effect waves-light btn" id="btnSignUp">Read Article</a>
    </div>
    {{/each}}
  </div>
</div>